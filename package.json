  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samehshi/portfolio.git"
  },
  "keywords": [
    "data-science",
    "portfolio",
    "business-intelligence",
    "analytics",
    "react"
  ],
  "private": true,
  "dependencies": {
    "colorthief": "^2.3.0",
    "dotenv": "^8.2.0",
    "gh-pages": "^6.3.0",
    "jest-canvas-mock": "^2.3.1",
    "lottie-react": "^2.4.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-easy-emoji": "^1.8.1",
    "react-ga4": "^2.1.0",
    "react-headroom": "^3.0.0",
    "react-helmet-async": "^2.0.5",
    "react-reveal": "^1.2.2",
    "react-scripts": "^5.0.0",
    "react-twitter-embed": "3.0.3",
    "sass": "^1.32.8",
    "sharp": "^0.34.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "start": "node fetch.js && react-scripts start",
    "build": "node fetch.js && react-scripts build",
    "deploy": "gh-pages -b gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./**/*.{js,css,json,scss}\"",
    "check-format": "prettier -c \"./**/*.{js,css,json,scss}\"",
    "format-staged": "prettier --write",
    "validate-build": "npm run check-format && npm run build",
    "pre-commit": "npm run format-staged",
    "health-check": "node validate-content.js && node validate-seo.js",
    "deployment-health": "node deployment-health-check.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "prettier": "^2.3.1"
  }
}
